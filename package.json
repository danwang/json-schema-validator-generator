{
  "name": "json-schema-validator-generator",
  "version": "1.1.8",
  "main": "dist/index.js",
  "scripts": {
    "flow": "flow",
    "lint": "eslint src",
    "prettier": "cat .prettier | xargs prettier --write",
    "prettier-check-stale": "cat .prettier | xargs prettier --list-different || (printf '\\nERROR: Please run \"yarn run prettier\" before commiting!\\n\\n' && exit 1)",
    "jest": "jest --config jestconfig.json",
    "build": "yarn transpile && yarn transpile-flow && yarn copy-flow && yarn copy-packagejson",
    "transpile": "BABEL_ENV='test' babel ./src --out-dir ./dist/ --ignore '**/__tests__/*.js'",
    "transpile-flow": "BABEL_ENV='flow' babel ./src --out-dir ./dist-flow/ --ignore '**/__tests__/*.js'",
    "copy-flow": "find dist-flow -type f -print | xargs -I {} rename -s \".js\" \".flow.js\" -s \"dist-flow\" \"dist\" {}",
    "copy-packagejson": "cp package.json ./dist",
    "test": "yarn flow && yarn lint && yarn prettier-check-stale && yarn jest"
  },
  "repository": "github:danwang/json-schema-validator-generator",
  "homepage": "https://github.com/danwang/json-schema-validator-generator",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-stripe": "^1.0.12",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "7.0.1",
    "flow-bin": "0.53.1",
    "jest": "^19.0.2",
    "json-pointer": "^0.6.0",
    "lambdanwang": "1.2.2",
    "lodash": "^4.17.4",
    "prettier": "^1.5.2"
  }
}
